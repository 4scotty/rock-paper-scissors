{
  "name": "rock-paper-scissors",
  "version": "0.0.0",
  "description": "Rock-Paper-Scissors game",
  "author": "iZonder <izonder@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/izonder/rock-paper-scissors.git"
  },
  "bugs": {
    "url": "https://github.com/izonder/rock-paper-scissors/issues"
  },
  "homepage": "https://github.com/izonder/rock-paper-scissors#readme",
  "scripts": {
    "start": "yarn && cross-env NODE_ENV=development webpack-dev-server --config ./webpack",
    "build": "yarn && cross-env NODE_ENV=production webpack --config ./webpack",
    "test": "yarn && yarn lint",
    "lint": "yarn && eslint ."
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-optional-chaining"
    ]
  },
  "pre-commit": [
    "lint"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "autoprefixer": "^9.0.1",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "^5.0.5",
    "css-loader": "^2.0.0",
    "eslint-config-standard": "izonder/eslint-config-standard#v2.2.0",
    "eslint-loader": "^2.0.0",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^3.0.0",
    "pre-commit": "^1.2.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.23.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.4"
  }
}
